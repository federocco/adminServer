{
  "server": {
    "port": "@@HTTP_PORT"
  },
  "database": {
    "host": "@@DB_HOSTADDRESS",
    "port": 3306,
    "username": "@@DB_USERNAME",
    "password": "@@DB_PASSWORD",
    "database": "@@DB_NAME",
    "dialect": "mysql",
    "define": {
      "paranoid": true,
      "timestamps": true,
      "underscored": false,
      "freezeTableName": true,
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "deletedAt": "deletedAt",
      "charset": "utf8",
      "schema": "public"
    }
  }
}
